<li class="dropdown">
    <a asp-controller="Drink" asp-action="Index" class="dropdown-toggle" data-toggle="dropdown">Drinks<b class="caret"></b></a>
    <ul class="dropdown-menu">
        @foreach (var category in Model)
        {
            <li><a asp-controller="Drink" asp-action="Index" asp-route-Category="@category.CategoryName">@category.CategoryName</a></li>
        }
        <li class="divider"></li>
        <li>
            <a asp-controller="Drink" asp-action="Index" asp-route-Category="">View All Drinks</a>
        </li>
    </ul>
</li>

